@article{APJL_Novo,
  author={A Stockem Novo and A Bret and U Sinha},
  title={Shock formation in magnetised electron\u2013positron plasmas: mechanism and timing},
  journal={New Journal of Physics},
  volume={18},
  number={10},
  pages={105002},
  url={http://stacks.iop.org/1367-2630/18/i=10/a=105002},
  year={2016},
  abstract={The shock formation process in electron\u2013positron pair plasmas is investigated in the presence of an ambient perpendicular magnetic field. In initially unmagnetised plasmas, which are dominated by the Weibel or filamentation instability, the shock formation time is a multiple of the saturation time of the linear instability. While in weakly magnetised plasmas the mechanism is still the same, higher magnetisations induce synchrotron maser modes such that the shock formation is dominated by magnetic reflection. As a consequence the formation times are reduced. The focus is on the detailed picture of the particle kinetics, in which the transition between Weibel and magneto-hydrodynamic shocks can be clearly identified.}
}
	

